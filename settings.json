{
    "python.pythonPath": "C:\\Python37\\python.exe",
    "workbench.colorTheme": "Dracula",
    "python.jediEnabled": true,
    "[python]": {
        
    },
    "[plaintext]": {
        "editor.wordWrap": "on"
    },
    "python.linting.pylintArgs": [
        "--enable=W0614",
        "--disable=C0301",
        "--disable=C0303",
        "--disable=C0325",
        "--disable=W0703",
        "--enable=W0612",
        "--disable=C0111",
        "--disable=C0413",
        "--disable=C0411",
        "--disable=C0103",
        "--disable=C0330",
        "--disable=W0312",
        "--disable=C0304",
        "--disable=W1401",
        "--disable=W0311",
        "--disable=C0121",
        "--disable=W0123",
        "--disable=W0614",
        "--disable=C0302",
        "--disable=C0412",
        "--disable=C0200",
        "--disable=W0401",
        "--disable=W0621",
        "--extension-pkg-whitelist=pyodbc"
    ],
    "python.linting.pylintEnabled": true,
    "workbench.iconTheme": "material-icon-theme",
    "terminal.integrated.scrollback": 50000,
    "git.autofetch": true,
    "bracketPairColorizer.forceUniqueOpeningColor": false,
    "todohighlight.isCaseSensitive": true,
    "todohighlight.keywords": [
        {
            "text": "TODO",
            "backgroundColor": "rgba(255,153,51,1)",
            "color": "#FFFFFF"
        },
        "FIXME",
        {
            "text": "NOTE",
            "backgroundColor": "rgba(214,224,226,1)"
        },
        {
            "text": "WORKING!",
            "backgroundColor": "rgba(1,201,16,1)",
            "color": "#FFFFFF"
        },
        {
            "text": "REMOVE",
            "backgroundColor": "rgba(198,0,5,1)",
            "color": "#FFFFFF"
        },
        {
            "text": "CHANGE",
            "backgroundColor": "rgba(255,255,13,1)",
            "color": "#0f8080"
        },
        {
            "text": "ENDING",
            "backgroundColor": "rgba(40,42,54,0)",
            "color": "#ff79c6"
        }
    ],
    "git.ignoreWindowsGit27Warning": true,
    "python.languageServer": "Jedi",
    "workbench.editorAssociations": [
        {
            "viewType": "jupyter.notebook.ipynb",
            "filenamePattern": "*.ipynb"
        }
    ],
    "azureFunctions.showCoreToolsWarning": false
}