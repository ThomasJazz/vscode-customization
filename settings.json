{
    "python.pythonPath": "C:\\Python37\\python.exe",
    "workbench.colorTheme": "Dracula",
    "[python]": {
        
    },
    "[plaintext]": {
        "editor.wordWrap": "on"
    },
    "python.linting.pylintArgs": [
        "--enable=W0614",
        "--disable=C0301",
        "--disable=C0303",
        "--disable=C0325",
        "--disable=W0703",
        "--enable=W0612",
        "--disable=C0111",
        "--disable=C0413",
        "--disable=C0411",
        "--disable=C0103",
        "--disable=C0330",
        "--disable=W0312",
        "--disable=C0304",
        "--disable=W1401",
        "--disable=W0311",
        "--disable=C0121",
        "--disable=W0123",
        "--disable=W0614",
        "--disable=C0302",
        "--disable=C0412",
        "--disable=C0200",
        "--disable=W0401",
        "--disable=W0621",
        "--extension-pkg-whitelist=pyodbc"
    ],
    "python.linting.pylintEnabled": true,
    "workbench.iconTheme": "material-icon-theme",
    "terminal.integrated.scrollback": 50000,
    "git.autofetch": true,
    "bracketPairColorizer.forceUniqueOpeningColor": false,
    "todohighlight.isCaseSensitive": true,
    "todohighlight.keywords": [
        {
            "text": "TODO:",
            "backgroundColor": "#ffb86c",
            "color": "#f8f8f2"
        },
        {
            "text": "FIXME:",
            "backgroundColor": "#8be9fd",
            "color": "#ff79c6"
        },
        {
            "text": "NOTE:",
            "backgroundColor": "#f8f8f2",
            "color": "#6272a4"
        },
        {
            "text": "WORKING:",
            "backgroundColor": "#50fa7b",
            "color": "#44475a"
        },
        {
            "text": "REMOVE:",
            "backgroundColor": "#ff5555",
            "color": "#f8f8f2"
        },
        {
            "text": "CHANGE:",
            "backgroundColor": "#f1fa8c",
            "color": "#0f8080"
        },
        {
            "text": "ENDING:",
            "backgroundColor": "#ff79c6",
            "color": "#f8f8f2"
        },
        {
            "text": "DEBUG:",
            "backgroundColor": "#bd93f9",
            "color": "#f1fa8c"
        }
    ],
    "git.ignoreWindowsGit27Warning": true,
    "python.languageServer": "Jedi",
    "workbench.editorAssociations": [
        {
            "viewType": "jupyter.notebook.ipynb",
            "filenamePattern": "*.ipynb"
        }
    ],
    "azureFunctions.showCoreToolsWarning": false
}